{
  "name": "@infuseai/piperider-ui",
  "author": {
    "name": "infuseai"
  },
  "homepage": "./",
  "description": "A UI library for building Piperider generated reports",
  "version": "0.1.0-alpha.1",
  "license": "MIT",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "@chakra-ui/icons": "^2.0.1",
    "@chakra-ui/react": "^2.1.2",
    "@chakra-ui/styled-system": "^2.1.1",
    "@chakra-ui/system": "^2.1.1",
    "@chakra-ui/theme-tools": "^2.0.1",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "usehooks-ts": "^2.6.0"
  },
  "devDependencies": {
    "@amplitude/analytics-browser": "^1.1.2",
    "@amplitude/analytics-types": "^0.6.1",
    "@babel/core": "^7.18.2",
    "@chakra-ui/icons": "^2.0.1",
    "@chakra-ui/react": "^2.1.2",
    "@chakra-ui/styled-system": "^2.1.1",
    "@chakra-ui/system": "^2.1.1",
    "@chakra-ui/theme-tools": "^2.0.1",
    "@craco/craco": "^6.4.5",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^14.0.1",
    "@rollup/plugin-typescript": "^8.5.0",
    "@sentry/browser": "^7.8.0",
    "@sentry/tracing": "^7.8.0",
    "@sgratzl/chartjs-chart-boxplot": "~3.8.0",
    "@types/jest": "^28.1.2",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "chalk": "^5.0.1",
    "chart.js": "~3.8.1",
    "chartjs-adapter-date-fns": "^2.0.0",
    "craco-esbuild": "^0.5.1",
    "cypress": "^10.3.0",
    "date-fns": "^2.28.0",
    "date-fns-tz": "^1.3.6",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "framer-motion": "^6.3.4",
    "http-server": "^14.1.1",
    "inter-ui": "^3.19.3",
    "json-schema-to-typescript": "^10.1.5",
    "nanoid": "^3.3.4",
    "node-html-parser": "^5.3.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "react": "^18.1.0",
    "react-chartjs-2": "^4.3.1",
    "react-dom": "^18.1.0",
    "react-icons": "^4.4.0",
    "react-scripts": "5.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.0",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "ts-to-zod": "^1.11.0",
    "typescript": "^4.7.4",
    "usehooks-ts": "^2.6.0",
    "web-vitals": "^2.1.4",
    "wouter": "2.8.0-alpha.2",
    "zod": "^3.17.10"
  },
  "scripts": {
    "analyze": "npx cra-bundle-analyzer",
    "extract:metadata": "node src/sdlc/extract-schema-metadata",
    "setup": "run-s typing:dts typing:zts extract:metadata embed:html lint:fix",
    "serve:single": "dotenv -e ./build-env/.env.single craco start",
    "serve:comparison": "dotenv -e ./build-env/.env.comparison craco start",
    "start:single": "run-s setup serve:single",
    "start:comparison": "run-s setup serve:comparison",
    "typing:dts": "npx json2ts ../piperider_cli/profiler/schema.json > src/sdlc/single-report-schema.ts",
    "typing:zts": "npx ts-to-zod src/sdlc/single-report-schema.ts src/sdlc/single-report-schema.z.ts",
    "embed:html": "node src/sdlc/embed-window-data",
    "strip:html": "node src/sdlc/clean-window-data",
    "rm:cli-reports": "rimraf ../piperider_cli/data/report/*",
    "build": "run-s strip:html rm:cli-reports && run-p -l build:single build:comparison",
    "build:single": "npm run bundle:norm:single && mv build/single-report ../piperider_cli/data/report/single-report",
    "build:comparison": "npm run bundle:norm:comparison && mv build/comparison-report ../piperider_cli/data/report/comparison-report",
    "bundle:norm:single": "dotenv -e ./build-env/.env.single craco build",
    "bundle:case:single-edge": "dotenv -e ./build-env/.env.single-edge craco build",
    "bundle:norm:comparison": "dotenv -e ./build-env/.env.comparison craco build",
    "bundle:case:comparison-asymmetric": "dotenv -e ./build-env/.env.comparison-asymmetric craco build",
    "bundle:e2e:single": "dotenv -e ./build-env/.env.single-e2e craco build",
    "bundle:e2e:comparison": "dotenv -e ./build-env/.env.comparison-e2e craco build",
    "bundle:e2e": "run-s -l bundle:e2e:*",
    "bundle:e2e:case": "run-s -l bundle:case:*",
    "serve:e2e:norm:single": "http-server ./build/single-report -p 3000 --silent",
    "serve:e2e:norm:comparison": "http-server ./build/comparison-report -p 3001 --silent",
    "serve:e2e:case:single-edge": "http-server ./build/single-edge-report -p 4000 --silent",
    "serve:e2e:case:comparison-asymmetric": "http-server ./build/comparison-asymmetric-report -p 4001 --silent",
    "dev:lib": "run-s rollup:lib link:lib",
    "rollup:lib": "rimraf dist && rollup -c",
    "link:lib": "cp ./package.json dist && cd dist && npm link && cd ..",
    "lint": "eslint src/",
    "lint:fix": "npm run lint -- --fix"
  },
  "resolutions": {
    "nth-check": "2.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:prettier/recommended"
    ]
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "packageManager": "yarn@3.2.1"
}
